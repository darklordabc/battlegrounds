						<DOTAAbilityImage class="GroundsLootIconContent Hide" id="SlotAbilityIcon" abilityname="rubick_null_field"/>
						<DOTAItemImage class="GroundsLootIconContent Hide" id="SlotItemIcon" itemname="item_blink" />
						<Image class="GroundsLootIconContent Hide" id="SlotMiscIcon" src="file://{images}/custom_game/my_image.png" />
					</Panel>
				</Panel>
<!-- 				<Label hittest="false" id="PickupMessage_Hero_Text" class="TitleFont" text="#OverthrowHeroPickup" />
				<Label hittest="false" id="PickupMessage_Text" class="TitleFont" text="#OverthrowTextPickup" />
				<Label hittest="false" id="PickupMessage_Item_Text" class="TitleFont" text="#OverthrowItemPickup" /> -->
			</Panel>
		</snippet>		
	</snippets>
	<Panel hittest="false" class="OverthrowNotification">
