			<Label hittest="false" id="AlertMessage_Delivery" class="TitleFont" text="" html="true" />
		</Panel>
		<Panel hittest="false" id="OverthrowItemNotification">

		</Panel>
		<Panel hittest="false" id="KillMessage" text="">
			<Label hittest="false" id="KillMessage_Hero" class="TitleFont" text="#KillMessageHero" />
			<Label hittest="false" id="KillMessage_Text" class="TitleFont" text="#KillMessageText" />
			<Label hittest="false" id="KillMessage_Bounty" class="TitleFont" text="#KillMessageBounty" />
