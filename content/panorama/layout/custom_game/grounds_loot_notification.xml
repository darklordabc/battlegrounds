			<Label hittest="false" id="AlertTimer_Text" class="TitleFont" text="" />
			<Label hittest="false" id="AlertTimer_Remaining" class="TitleFont" text="#Remaining" />
		</Panel>
		<Panel hittest="false" id="Overtime" text="">
			<Label hittest="false" id="Overtime_Text" class="TitleFont" text="#Overtime" />
